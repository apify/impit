{
  "name": "impit",
  "homepage": "https://apify.github.io/impit/",
  "version": "0.1.5",
  "main": "index.js",
  "types": "index.d.ts",
  "napi": {
    "binaryName": "impit-node",
    "targets": [
      "x86_64-apple-darwin",
      "x86_64-unknown-linux-gnu",
      "x86_64-pc-windows-msvc",
      "aarch64-apple-darwin",
      "aarch64-pc-windows-msvc",
      "x86_64-unknown-linux-musl"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/apify/impit.git"
  },
  "license": "MIT",
  "devDependencies": {
    "@napi-rs/cli": "^3.0.0-alpha.70",
    "@types/node": "^22.13.1",
    "vitest": "^3.0.5"
  },
  "ava": {
    "timeout": "3m"
  },
  "engines": {
    "node": ">= 20"
  },
  "scripts": {
    "artifacts": "napi artifacts --dir ../artifacts --dist npm",
    "build": "napi build --platform --release",
    "build:debug": "napi build --platform",
    "prepublishOnly": "napi prepublish -t npm",
    "test": "vitest",
    "universal": "napi universal",
    "version": "napi version"
  },
  "packageManager": "yarn@4.6.0",
  "description": "Impit for JavaScript"
}
